{
  "compilerOptions": {
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES5",
    "lib": [
      "dom",
      "es2015",
      "es2015.promise"
    ],
    "removeComments": true,
    "allowSyntheticDefaultImports": false,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./src/Components/*",
        "./src/Containers/*"
      ]
    }
  },
    "exclude": [
      "node_modules",
  ],
  "awesomeTypescriptLoaderOptions": {
    "transpileOnly": true
  }
}
